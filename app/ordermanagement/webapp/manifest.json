{
  "_version": "1.76.0",
  "sap.app": {
    "id": "ordermanagement.ordermanagement",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "0.0.1"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "resources": "resources.json",
    "sourceTemplate": {
      "id": "@sap/generator-fiori:basic",
      "version": "1.20.0",
      "toolsId": "9e38aa20-8dbc-49de-9f32-2d25efb8e3e3"
    },
    "dataSources": {
      "mainService": {
        "uri": "/odata/v4/festival/",
        "type": "OData",
        "settings": {
          "annotations": [],
          "odataVersion": "4.0"
        }
      }
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "flexEnabled": false,
    "dependencies": {
      "minUI5Version": "1.143.2",
      "libs": {
        "sap.m": {},
        "sap.ui.core": {},
        "sap.f": {},
        "sap.uxap": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "ordermanagement.ordermanagement.i18n.i18n"
        }
      },
      "": {
        "dataSource": "mainService",
        "preload": true,
        "settings": {
          "operationMode": "Server",
          "autoExpandSelect": true,
          "earlyRequests": true
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "controlAggregation": "pages",
        "controlId": "app",
        "transition": "slide",
        "type": "View",
        "viewType": "XML",
        "path": "ordermanagement.ordermanagement.view",
        "async": true,
        "viewPath": "ordermanagement.ordermanagement.view"
      },
      "routes": [
        {
          "name": "RouteOrderManagement",
          "pattern": ":?query:",
          "target": [
            "TargetOrderManagement"
          ]
        }
      ],
      "targets": {
        "TargetOrderManagement": {
          "id": "OrderManagement",
          "name": "OrderManagement"
        }
      }
    },
    "rootView": {
      "viewName": "ordermanagement.ordermanagement.view.App",
      "type": "XML",
      "id": "App",
      "async": true
    }
  }
}
